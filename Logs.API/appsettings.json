{
  "MongoDbConfigurations": {
    "ConnectionString": "mongodb://mongoDb:27017",
    "DatabaseName": "LogsAPI",
    "LogsCollectionName": "Logs"
  },

  "ElasticConfigurations": {
    "BaseUri": "http://host.docker.internal:9200/",
    "DefaultIndex": "logsapi"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "JWTBearerConfiguration": {
    "Authority": "http://host.docker.internal:8020"
  },

  "Messages": {
    "UpdateOfficeEndpoint": "exchange:UpdateOffice",
    "DisableOfficeEndpoint": "exchange:DisableOffice"
  }
}
